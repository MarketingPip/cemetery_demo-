---
layout: none
permalink: /authors.json
---
{
  "authors": [
    {% for author in site.authors %}
      {
        "username": "{{ author.slug }}",
        "name": "{{ author.name }}",
        "author_name": "{{ author.author_name }}",
        "image": "{{ author.author_image }}"
      }
      {% if forloop.last == false %},{% endif %}
    {% endfor %}
  ]
}
