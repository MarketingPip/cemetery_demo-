# Gemfile

source 'https://rubygems.org' # This is where Bundler will fetch gems from.

# Use the Jekyll gem to build your site.
gem 'jekyll', '~> 4.0'

# Optional: Use the `jekyll-assets` gem for asset management (CSS/JS).
gem 'jekyll-assets'

# Optional: Use `jekyll-asset-minifier` for asset minification.
gem 'jekyll-minifier'
gem 'jekyll-sitemap'
gem 'jekyll-seo-tag'
gem 'httpparty'
gem 'jekyll-datapage-generator'

